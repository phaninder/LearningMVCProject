@model List<IGrouping<string, MVCApp4.Models.Employee>>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Grouping</title>
</head>
<body>
	<div>
		<p> Group</p>
		@foreach (var item in Model)
		{
			<table>
				<caption>@item.Key</caption>
				<tr>
					<th>EmpID</th>
					<th>EmpName</th>
					<th>EmpSalary</th>
					<th>EmpJob</th>
				</tr>
				@foreach (var item2 in item)
				{
					<tr>
						<td>@item2.EmpId </td>
						<td>@item2.EmpName </td>
						<td>@item2.Salary</td>
						<td>@item2.Job</td>
					</tr>
				}
			</table>
		}
	</div>
</body>
</html>